{% extends 'base.html.twig' %}

{% block title %}Changer votre mot de passe{% endblock %}

{% block content %}

{% for key, value in app.flashes %}
{% if  key == 'success' %}
    <div class="alert alert-success">
        {{ value[0] }}
    </div>
{% elseif key == 'failed' %}
    <div class="alert alert-danger">
        {{ value[0] }}
    </div>

{% endif  %}
{% endfor %}

{{ form(form) }}

{# {{ dump(app.flashes) }} #}
{% endblock %}
